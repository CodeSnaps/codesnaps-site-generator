    "i18next-browser-languagedetector": "^7.1.0",
    "i18next-http-backend": "^2.2.2",
    "i18next-resources-to-backend": "^1.1.4",
    "nanoid": "^5.0.1",
    "next": "13.4.19",
    "next-contentlayer": "0.3.4",
    "next-sitemap": "^4.2.3",
    "nodemailer": "^6.9.5",
    "pino": "^8.15.1",
    "react": "18.2.0",
    "react-dom": "18.2.0",
