    "i18next-http-backend": "^2.2.0",
    "i18next-resources-to-backend": "^1.1.3",
    "next": "13.2.3",
    "nodemailer": "^6.9.1",
    "pino": "^8.11.0",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-hook-form": "^7.43.7",
    "react-hot-toast": "^2.4.0",
    "react-i18next": "^12.2.0",
    "react-top-loading-bar": "^2.3.1",
    "recharts": "^2.5.0",
    "stripe": "^11.16.0",
