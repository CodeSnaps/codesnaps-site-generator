    "cypress": "^12.8.0",
    "encoding": "^0.1.13",
    "eslint": "^8.36.0",
    "eslint-config-next": "13.2.4",
    "eslint-config-prettier": "^8.8.0",
    "eslint-plugin-cypress": "^2.12.1",
    "pino-pretty": "^10.0.0",
    "postcss": "^8.4.21",
    "prettier": "2.8.6",
    "prettier-plugin-tailwindcss": "^0.2.5",
    "supabase": "1.45.2",
    "tailwindcss": "^3.2.7",
    "typescript": "4.9.5"
  }
